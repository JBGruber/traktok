citHeader("To cite traktok in publications use:")

citEntry(
  entry    = "Manual",
  title    = "traktok. Getting TikTok data through the official and unofficial APIs",
  author   = "Johannes B. Gruber",
  year     = 2023,
  url      = "https://github.com/JBGruber/traktok",
  note     = "R package version 0.0.4.9000",
  textVersion = paste(
    "Gruber, Johannes B. (2023). traktok. An R package to scrape data from TikTok. R package version 0.0.4.9000. https://github.com/JBGruber/traktok."
  )
)
